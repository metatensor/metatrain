{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "enum": [
        "pet"
      ]
    },
    "model": {
      "type": "object",
      "properties": {},
      "additionalProperties": false
    },
    "training": {
      "type": "object",
      "properties": {
        "batch_size": {
          "type": "integer"
        }
      },
      "additionalProperties": false
    },
    "atomic_types": {
      "type": "array",
      "items": {
        "type": "integer",
        "minimum": 1
      },
      "uniqueItems": true
    }
  }
}
